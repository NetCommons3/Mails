{
    "name": "netcommons/mails",
    "description": "Mails Plugin for NetCommons",
    "homepage": "http://www.netcommons.org/",
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations":            "~2.2",
        "netcommons/blocks":            "dev-master",
        "netcommons/net-commons":       "dev-master",
        "netcommons/pages":             "dev-master",
        "netcommons/roles":             "dev-master",
        "netcommons/rooms":             "dev-master",
        "netcommons/site-manager":      "dev-master",
        "netcommons/users":             "dev-master",
        "netcommons/workflow":          "dev-master"
    },
    "license": "NetCommons License",
    "authors": [
        {
          "name": "Mitsuru Mutaguchi(OpenSource WorkShop)",
          "email": "mutaguchi@opensource-workshop.jp",
          "homepage": "https://opensource-workshop.jp/",
          "role": "Developer"
        },
        {
            "name": "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/Mails/graphs/contributors"
        }
    ],
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "mails"],
    "config": {
        "vendor-dir": "vendors"
    }
}
